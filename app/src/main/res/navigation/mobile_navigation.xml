<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="ruemelin.de.bgm.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_team"
        android:name="ruemelin.de.bgm.ui.dashboard.DashboardFragment"
        android:label="@string/title_team"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_impressum"
        android:name="ruemelin.de.bgm.ui.notifications.NotificationsFragment"
        android:label="@string/title_impressum"
        tools:layout="@layout/fragment_notifications" />
</navigation>
